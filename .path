# Add `~/bin` to the `$PATH`
export PATH="$HOME/bin:$PATH";

# Homebrew
# Flutter
export PATH="$HOME/flutter/bin:$PATH"
export PATH=$(brew --prefix)/bin:$(brew --prefix)/sbin:$PATH

# Android SDK
export ANDROID_SDK_ROOT="/Users/mbritto/Library/Android/sdk"
export PATH="$PATH:$ANDROID_SDK_ROOT/tools:$ANDROID_SDK_ROOT/tools/bin:$ANDROID_SDK_ROOT/platform-tools"

# Apple Dev tools
export PATH="$PATH:/Library/Apple/usr/bin/"

# Python libs 
export PATH=$PATH:/Users/mbritto/Library/Python/3.7/bin
export PATH=$HOME/.pyenv/shims:$PATH

# Dart pub extensions
export PATH="$PATH":"$HOME/.pub-cache/bin"